{
  "name": "transaction-audit-pipeline",
  "type": "DataPipeline",
  "activities": [
    {
      "name": "IngestTransactions",
      "type": "Notebook",
      "notebook": {
        "path": "/databricks/notebooks/transaction_audit_ingestion.py"
      }
